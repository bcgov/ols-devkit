<?xml version="1.0"?>
<kml
  xmlns="http://www.opengis.net/kml/2.2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/kml/2.2  http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd ">
  <Document>
    <Style id="geomarkDefault">
 		  <BalloonStyle>
		   <text>
         <![CDATA[
             <p ALIGN="center"><b>$[name]</b></p>
             <p ALIGN="center"><a href="mailto:FrontCounterBC@gov.bc.ca?subject=This geomark represents the spatial extent of my tenure or right application&body=My name is yourFirstName yourLastName. The spatial extent of my tenure or right application is $[url]" target="_top">Email to FrontCounterBC</a></p>
             <p ALIGN="center"><a href="$[url]" target="_blank">View in web browser</a></p>
		         <tbody>
		         <table>
		           <tr> <td align="right"><i>Created: </i></td>   <td>$[createDate] </td> </tr>
		           <tr> <td align="right"><i>Expiry Date: </i></td>   <td>$[expiryDate] </td> </tr>
               <tr> <td align="right"><i>Bounding Box (W,S,E,N): </i></td>   <td>$[minY],$[minX],<br>$[maxY],$[maxX]</td> </tr>
               <tr> <td align="right"><i>Centroid(lat,lon): </i></td>   <td>$[centroidY],$[centroidX]</td> </tr>
               <tr> <td align="right"><i>Geometry Type: </i></td>   <td>$[geometryType] </td> </tr>
		           <tr> <td align="right"><i>Part Number: </i></td>   <td>$[partIndex] </td> </tr>
		           <tr> <td align="right"><i>Geometry Count: </i></td>   <td>$[numParts] </td> </tr>
               <tr> <td align="right"><i>Vertex Count: </i></td>   <td>$[numVertices] </td> </tr>
               <tr> <td align="right"><i>Length: </i></td>   <td>$[length] </td> </tr>
               <tr> <td align="right"><i>Area: </i></td>   <td>$[area] </td> </tr>
               <tr> <td align="right"><i>Is Valid: </i></td>   <td>$[isValid] </td> </tr>
               <tr> <td align="right"><i>Is Simple: </i></td>   <td>$[isSimple] </td> </tr>
               <tr> <td align="right"><i>Is Robust: </i></td>   <td>$[isRobust] </td> </tr>
               <tr> <td align="right"><i>MinimumClearance: </i></td>   <td>$[minimumClearance] </td> </tr>
             </table>
		         </tbody>
         ]]>
       </text>
 		  </BalloonStyle> 

      <IconStyle>
			<color>ffffaaff</color>
			<scale>1.4</scale>
        <Icon>
				<href>http://maps.google.com/mapfiles/kml/paddle/pink-blank.png</href>
        </Icon>
        <hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
      </IconStyle>
		<LineStyle>
			<color>ff0000ff</color>
			<width>3</width>
		</LineStyle>
		<PolyStyle>
			<color>400000ff</color>
		</PolyStyle>
 
    </Style>
 
  </Document>

</kml>
