<!DOCTYPE html><html xmlns="xml" lang="en">
<head>
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title>Geomark Web Service REST API</title>
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.css" rel="stylesheet" type="text/css" />
<link href="bootstrap-custom.css" rel="stylesheet" type="text/css" />
<link href="../../css/rs.css" rel="stylesheet" type="text/css" />
<style type="text/css">
body{padding-top:60px}
*[id]:before {display:block;content:' ';margin-top:-75px;height:75px;visibility:hidden;}
</style>
</head>
<body data-spy="scroll" data-target="#navMain" data-offset="60">
<nav id="navMain" class="navbar navbar-default navbar-fixed-top">
<div class="container">
<div class="navbar-header">
<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
<span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button>
<div class="navbar-brand"><a href="#"><span class="navbar-brand-title">Geomark Web Service REST API</span></a></div></div>
<div id="navbar" class="navbar-collapse collapse" aria-expanded="false">
  <ul class="nav navbar-nav">
    <for each="thisClass" in="data">
      <if test="thisClass.alias != '_global_'">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">{+ thisClass.alias +}<span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><a href="#{+ thisClass.alias +}">{+ thisClass.alias +}</a></li>
            <li class="divider"> </li>
            <if test="!thisClass.isBuiltin() && (thisClass.isNamespace || thisClass.is('CONSTRUCTOR'))">
              <!-- constructor -->
              <li><a href="#{+ thisClass.alias +}.{+ thisClass.alias +}">{+ thisClass.alias +}</a></li>
            </if>  
            <if test="thisClass.methods.length">
              {! var ownMethods = thisClass.methods.filter(function($){return $.memberOf == thisClass.alias  && !$.isNamespace}).sort(makeSortby("name")); !}
            
              <if test="defined(ownMethods) && ownMethods.length">
                <for each="member" in="ownMethods">
                  <li><a href="#{+ thisClass.alias +}.{+ member.name +}">{+ member.name.replace(/\^\d+$/, '') +}</a></li>
                </for>
              </if>
            </if>
          </ul>
        </li>
      </if>
    </for>
  </ul>
</div>
</div>
</nav>
<div class="container-fluid">
<h1>Geomark Web Service - JavaScript API</h1>
<for each="thisClass" in="data">
  <if test="thisClass.alias != '_global_'">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h2 id="{+ thisClass.alias +}" class="panel-title"><small>class</small> {+ thisClass.alias +}</h2>
      </div>
      <div class="panel-body">
        <if test="!thisClass.isBuiltin() && (thisClass.isNamespace || thisClass.is('CONSTRUCTOR'))">
          <div class="panel panel-info">
            <div class="panel-heading">
              <h4 id="{+ thisClass.alias +}.{+ thisClass.alias +}" class="panel-title">{+ thisClass.alias +}<small>{+ makeSignature(thisClass.params) +}</small></h4>
            </div>
            <div class="panel-body">
{+ thisClass.desc +}
              <if test="thisClass.params.length">
                <div class="table-responsive">
                  <table class="table table-striped table-bordered table-condensed">
                    <thead>
                      <tr>
                        <th>Parameter</th>
                        <th>Type</th>
                        <th>Description</th>
                      </tr>
                    </thead>
                    <tbody>
                      <for each="item" in="thisClass.params">
                      <tr>
                        <td>{+ item.name +}</td>
                        <td>{+ item.type +}</td>
                        <td>{+ item.desc +}</td>
                      </tr>
                      </for>
                    </tbody>
                  </table>
                </div>
              </if>
            </div>
          </div>
        </if>  
        <if test="thisClass.methods.length">
          {! var ownMethods = thisClass.methods.filter(function($){return $.memberOf == thisClass.alias  && !$.isNamespace}).sort(makeSortby("name")); !}
        
          <if test="defined(ownMethods) && ownMethods.length">
            <for each="member" in="ownMethods">
              <div class="panel panel-info">
                <div class="panel-heading">
                <h4 id="{+ thisClass.alias +}.{+ member.name +}" class="panel-title">{+ member.name.replace(/\^\d+$/, '') +}<small>{+ makeSignature(thisClass.params) +}</small></h4>
                </div>
                <div class="panel-body">
                  {+ member.desc +}
                  <if test="member.params.length">
                    <div class="table-responsive">
                      <table class="table table-striped table-bordered table-condensed">
                        <thead>
                          <tr>
                            <th>Parameter</th>
                            <th>Type</th>
                            <th>Description</th>
                          </tr>
                        </thead>
                        <tbody>
                          <for each="item" in="thisClass.params">
                          <tr>
                            <td>{+ item.name +}</td>
                            <td>{+ item.type +}</td>
                            <td>{+ item.desc +}</td>
                          </tr>
                          </for>
                        </tbody>
                      </table>
                    </div>
                  </if>
                </div>
              </div>
            </for>
          </if>
        </if>
      </div>
    </div>
  </if>
</for>
</div>

<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js">
</script>
<script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js">
</script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.js">
</script>
<script type="text/javascript" src="../../js/geomark.js">
</script>
</body>
</html>
